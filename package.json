{
  "name": "modern-v",
  "version": "0.1.6",
  "description": "Most easy way to get and set the mode in any situation.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "rollup": "rollup -c",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/just-do-halee/modern-v.git"
  },
  "keywords": [
    "env",
    "mode",
    "simple",
    "typescript",
    "NODE_ENV",
    "development",
    "production",
    "test",
    "application",
    "configuration",
    "config"
  ],
  "author": "just-do-halee <just.do.halee@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/just-do-halee/modern-v/issues"
  },
  "homepage": "https://github.com/just-do-halee/modern-v#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.17",
    "dotenv": "^16.0.0",
    "jest": "^28.1.0",
    "rollup": "^2.66.1",
    "rollup-plugin-dts": "^4.1.0",
    "ts-jest": "^28.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "tests",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
